<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.agendadecontatos">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".TelaSplash"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_tela_splash"
            android:theme="@style/FullscreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TelaLogin"
            android:label="Tela login"></activity>
        <activity
            android:name=".ConsultaContato"
            android:label="Consulta Contato" />
        <activity
            android:name=".CadastrarContato"
            android:label="Cadastro Contato" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>